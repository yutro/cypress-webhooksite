{
  "name": "@icokie/cypress-webhooksite",
  "version": "1.0.0",
  "description": "Simple library that adds webhook.site commands for email testing with cypress.io",
  "main": "lib/commands.js",
  "types": "types/index.d.ts",
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "files": [
    "lib/*",
    "types"
  ],
  "scripts": {
    "build": "tsc -b ./tsconfig.json ./tsconfig.tasks.json",
    "prebuld": "npx rimraf lib",
    "prepare": "npm run build",
    "version": "git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Icokie/cypress-webhooksite.git"
  },
  "keywords": [
    "webhook.site",
    "cypress webhook.site commands",
    "cypress email testing",
    "cypress email commands",
    "cypress plugins"
  ],
  "author": "icokie (Yury Troynov)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Icokie/cypress-webhooksite/issues"
  },
  "homepage": "https://github.com/Icokie/cypress-webhooksite#readme",
  "devDependencies": {
    "typescript": "5.0.4",
    "cypress": "12.10.0"
  },
  "peerDependencies": {
    "cypress": "^12.*"
  }
}
